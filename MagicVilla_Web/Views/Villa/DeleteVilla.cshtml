@model DtoVillaGet

<form asp-action="DeleteVilla">
	<br />
	<div class="container border p-3">
		<h1 class="text-primary">Delete Villa</h1>
		<hr />
		@Html.AntiForgeryToken()
		<div class="row">
			<div class="col-2">
				<label asp-for="Name" class="control-label pt-2" style="font-size:20px;"></label>
			</div>
			<div class="col-10 pb-3">
				<input disabled class="form-control" asp-for="Name" />
			</div>

			<div class="col-2">
				<label asp-for="Rate" class="control-label pt-2" style="font-size:20px;"></label>
			</div>
			<div class="col-10 pb-3">
				<input disabled class="form-control" asp-for="Rate" />
			</div>


			<div class="col-2">
				<label asp-for="Details" class="control-label pt-2" style="font-size:20px;"></label>
			</div>
			<div class="col-10 pb-3">
				<textarea class="form-control"disabled rows="5" asp-for="Details"></textarea>
			</div>
			<div class="col-2">
				<label asp-for="ImageUrl" class="control-label pt-2" style="font-size:20px;"></label>
			</div>
			<div class="col-10 pb-3">
				<input class="form-control" disabled asp-for="ImageUrl" />
			</div>
			<div class="col-2">
				<label asp-for="Occupancy" class="control-label pt-2" style="font-size:20px;"></label>
			</div>
			<div class="col-10 pb-3">
				<input class="form-control" disabled asp-for="Occupancy" />
			</div>
			<div class="col-2">
				<label asp-for="Sqft" class="control-label pt-2" style="font-size:20px;"></label>
			</div>
			<div class="col-10 pb-3">
				<input class="form-control" disabled asp-for="Sqft" />
			</div>
			<div class="col-2">
				<label asp-for="Amenity" class="control-label pt-2" style="font-size:20px;"></label>
			</div>
			<div class="col-10 pb-3">
				<textarea rows="5" class="form-control" disabled asp-for="Amenity"></textarea>
			</div>
			<div class="col-5 offset-2">
				<a asp-action="Index" class="btn btn-primary form-control">Back To List</a>
			</div>
			<div class="col-5">

				<input type="submit" asp-route-Id="@Model.Id" value="Delete" class="btn btn-danger form-control" />
			</div>
		</div>

	</div>
</form>
@section Scripts {
	<partial name="_ValidationScriptsPartial" />
}